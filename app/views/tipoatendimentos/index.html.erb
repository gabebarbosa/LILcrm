<p id="notice"><%= notice %></p>


<div class="panel panel-default">
  <div class="panel-body">
    <div class="row">
      <div class="col-md-9">
        <% button_to "Novo Tipo de atendimento", new_tipoatendimento_path, :method => "get", class: "btn btn-default btn-xs" %>
        <% link_to 'Novo Tipo de atendimento', new_tipoatendimento_path %>
        <%= link_to " Novo Tipo de atendimento", new_tipoatendimento_path, :method => "get", class: " fa fa-plus-circle btn btn-default btn-xs"%> 
      </div>
      <div align="right" class="col-md-3">
        <%= form_tag tipoatendimentos_path, method: :get do  %>
          <div class="input-group input-group-sm">
              <span class="input-group-addon" id="sizing-addon3"><i class="fa fa-search label-tema" aria-hidden="true"></i></span>
              <%= text_field_tag :descricao, @descricao_buscar, class: "form-control", placeholder: "buscar tipos de atendimentos ... " %>
              <% button_to "Buscar", nil, class: "fa fa-search btn btn-defaut btn-xs" %>
          </div>  
        <% end %>
      </div>

    </div>
  </div>
</div>


<table class="table table-condensed table-hover">
  <tbody>
    <% @tipoatendimentos.each do |tipoatendimento| %>
      <tr>
        <td><i class="fa fa-bookmark label-tema" aria-hidden="true"></i> <%= tipoatendimento.descricao %></td>
        <td align="right">
          <%= link_to ' Editar', edit_tipoatendimento_path(tipoatendimento), class: " fa fa-pencil-square-o btn btn-default btn-xs" %>
          <% link_to 'Ver', tipoatendimento %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>



